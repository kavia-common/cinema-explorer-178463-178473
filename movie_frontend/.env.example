# React (CRA) environment variables
# Copy this file to .env.local and fill in your values.
# Note: Do not commit .env.local; it contains secrets.

# Supabase project URL and anon (public) key
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
REACT_APP_SUPABASE_KEY=YOUR_PUBLIC_ANON_KEY

# Site URL used for auth redirects (e.g., http://localhost:3000)
REACT_APP_SITE_URL=http://localhost:3000

# TMDB API key or v4 read access token (used by frontend TMDB integration)
REACT_APP_TMDB_API_KEY=YOUR_TMDB_V4_READ_ACCESS_TOKEN_OR_V3_KEY
