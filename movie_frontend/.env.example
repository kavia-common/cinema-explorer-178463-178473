# React (CRA) environment variables for Movie Frontend
# Copy this file to .env.local and fill in your values.

# Supabase
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
REACT_APP_SUPABASE_KEY=YOUR_PUBLIC_ANON_KEY

# TMDB API key or v4 read access token
REACT_APP_TMDB_API_KEY=YOUR_TMDB_V4_READ_ACCESS_TOKEN_OR_V3_KEY

# Site URL used for auth redirects
REACT_APP_SITE_URL=http://localhost:3000
